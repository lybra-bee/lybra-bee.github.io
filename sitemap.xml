---
layout: null
sitemap:
  priority: 1.0
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  <!-- Главная страница -->
  <url>
    <loc>https://lybra-ai.ru/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>1.0</priority>
    <changefreq>weekly</changefreq>
  </url>

  <!-- Основные разделы сайта -->
  <url>
    <loc>https://lybra-ai.ru/about/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
    <changefreq>monthly</changefreq>
  </url>
  <url>
    <loc>https://lybra-ai.ru/articles/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.9</priority>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>https://lybra-ai.ru/gallery/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.7</priority>
    <changefreq>monthly</changefreq>
  </url>

  <!-- Все посты основного сайта -->
  {% for post in site.posts %}
  <url>
    <loc>https://lybra-ai.ru{{ post.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
    <changefreq>monthly</changefreq>
  </url>
  {% endfor %}

  <!-- Лаборатория Lybra AI Lab -->
  <url>
    <loc>https://lybra-ai.ru/lybra-ai-lab/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>1.0</priority>
    <changefreq>weekly</changefreq>
  </url>

  <!-- Эксперименты лаборатории -->
  {% for experiment in site.lybra-ai-lab.experiments %}
  <url>
    <loc>https://lybra-ai.ru/lybra-ai-lab{{ experiment.url }}</loc>
    <lastmod>{{ experiment.date | date_to_xmlschema }}</lastmod>
    <priority>0.9</priority>
    <changefreq>weekly</changefreq>
  </url>
  {% endfor %}

</urlset>
